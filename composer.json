{
    "name": "besmartand-pro/ups-api",
    "description": "PHP SDK for UPS REST API",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "BeSmartAnd.pro",
            "email": "kuba@besmartand.pro"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "jane-php/open-api-runtime": "^7.5",
        "php-http/curl-client": "^2.3",
        "symfony/polyfill-php81": "^1.28",
        "symfony/serializer": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "jane-php/open-api-3": "^7.5",
        "nyholm/psr7": "^1.8",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "BesmartandPro\\UpsApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        },
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    }
}
